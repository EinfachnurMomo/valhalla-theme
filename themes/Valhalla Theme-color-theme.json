{
	"name": "Valhalla Theme",
	"type": "dark",
	"colors": {
		// Contrast colors
		"contrastActiveBorder": "#ff0000", // An extra border around active elements to separate them from others for greater contrast.
		"contrastBorder": "#ff0000", // An extra border around elements to separate them from others for greater contrast.

		// Base colors
		"focusBorder": "#ff0000", // Overall border color for focused elements. This color is only used if not overridden by a component.
		"foreground": "#ff0000", // Overall foreground color. This color is only used if not overridden by a component.
		"widget.shadow": "#ff0000", // Shadow color of widgets such as Find/Replace inside the editor.
		"selection.background": "#ff0000", // Background color of text selections in the workbench (for input fields or text areas, does not apply to selections within the editor and the terminal).
		"descriptionForeground": "#ff0000", // Foreground color for description text providing additional information, for example for a label.
		"errorForeground": "#ff0000", // Overall foreground color for error messages (this color is only used if not overridden by a component).
		"icon.foreground": "#ff0000", // The default color for icons in the workbench.
		"sash.hoverBorder": "#ff0000", // The hover border color for draggable sashes.

		// Window border
		"window.activeBorder": "#ff0000", // Border color for the active (focused) window.
		"window.inactiveBorder": "#ff0000", // Border color for the inactive (unfocused) windows.

		// Text colors
		"textBlockQuote.background": "#ff0000", // Background color for block quotes in text.
		"textBlockQuote.border": "#ff0000", // Border color for block quotes in text.
		"textCodeBlock.background": "#ff0000", // Background color for code blocks in text.
		"textLink.activeForeground": "#ff0000", // Foreground color for links in text when clicked on and on mouse hover.
		"textLink.foreground": "#ff0000", // Foreground color for links in text.
		"textPreformat.foreground": "#ff0000", // Foreground color for preformatted text segments.
		"textSeparator.foreground": "#ff0000", // Color for text separators.

		// Action colors
		"toolbar.hoverBackground": "#ff0000", // Toolbar background when hovering over actions using the mouse
		"toolbar.hoverOutline": "#ff0000", // Toolbar outline when hovering over actions using the mouse
		"toolbar.activeBackground": "#ff0000", // Toolbar background when holding the mouse over actions
				
		// Button control
		"button.background": "#ff0000", // Button background color.
		"button.foreground": "#ff0000", // Button foreground color.
		"button.border": "#ff0000", // Button border color.
		"button.hoverBackground": "#ff0000", // Button background color when hovering.
		"button.secondaryForeground": "#ff0000", // Secondary button foreground color.
		"button.secondaryBackground": "#ff0000", // Secondary button background color.
		"button.secondaryHoverBackground": "#ff0000", // Secondary button background color when hovering.
		"checkbox.background": "#ff0000", // Background color of checkbox widget.
		"checkbox.foreground": "#ff0000", // Foreground color of checkbox widget.
		"checkbox.border": "#ff0000", // Border color of checkbox widget.

		// Dropdown control
		"dropdown.background": "#ff0000", // Dropdown background.
		"dropdown.listBackground": "#ff0000", // Dropdown list background.
		"dropdown.border": "#ff0000", // Dropdown border.
		"dropdown.foreground": "#ff0000", // Dropdown foreground.

		// Input control
		"input.background": "#ff0000", // Input box background.
		"input.border": "#ff0000", // Input box border.
		"input.foreground": "#ff0000", // Input box foreground.
		"input.placeholderForeground": "#ff0000", // Input box foreground color for placeholder text.
		"inputOption.activeBackground": "#ff0000", // Background color of activated options in input fields.
		"inputOption.activeBorder": "#ff0000", // Border color of activated options in input fields.
		"inputOption.activeForeground": "#ff0000", // Foreground color of activated options in input fields.
		"inputOption.hoverBackground": "#ff0000", // Background color of activated options in input fields.
		"inputValidation.errorBackground": "#ff0000", // Input validation background color for error severity.
		"inputValidation.errorForeground": "#ff0000", // Input validation foreground color for error severity.
		"inputValidation.errorBorder": "#ff0000", // Input validation border color for error severity.
		"inputValidation.infoBackground": "#ff0000", // Input validation background color for information severity.
		"inputValidation.infoForeground": "#ff0000", // Input validation foreground color for information severity.
		"inputValidation.infoBorder": "#ff0000", // Input validation border color for information severity.
		"inputValidation.warningBackground": "#ff0000", // Input validation background color for information warning.
		"inputValidation.warningForeground": "#ff0000", // Input validation foreground color for warning severity.
		"inputValidation.warningBorder": "#ff0000", // Input validation border color for warning severity.

		// Scrollbar control
		"scrollbar.shadow": "#ff0000", // Scrollbar slider shadow to indicate that the view is scrolled.
		"scrollbarSlider.activeBackground": "#ff0000", // Scrollbar slider background color when clicked on.
		"scrollbarSlider.background": "#ff0000", // Scrollbar slider background color.
		"scrollbarSlider.hoverBackground": "#ff0000", // Scrollbar slider background color when hovering.

		// Badge
		"badge.foreground": "#ff0000", // Badge foreground color.
		"badge.background": "#ff0000", // Badge background color.

		// Progress bar
		"progressBar.background": "#ff0000", // Background color of the progress bar shown for long running operations.

		// Lists and trees
		"list.activeSelectionBackground": "#ff0000", // List/Tree background color for the selected item when the list/tree is active.
		"list.activeSelectionForeground": "#ff0000", // List/Tree foreground color for the selected item when the list/tree is active.
		"list.activeSelectionIconForeground": "#ff0000", // List/Tree icon foreground color for the selected item when the list/tree is active. An active list/tree has keyboard focus, an inactive does not.
		"list.dropBackground": "#ff0000", // List/Tree drag and drop background when moving items around using the mouse.
		"list.focusBackground": "#ff0000", // List/Tree background color for the focused item when the list/tree is active.
		"list.focusForeground": "#ff0000", // List/Tree foreground color for the focused item when the list/tree is active. An active list/tree has keyboard focus, an inactive does not.
		"list.focusHighlightForeground": "#ff0000", // List/Tree foreground color of the match highlights on actively focused items when searching inside the list/tree.
		"list.focusOutline": "#ff0000", // List/Tree outline color for the focused item when the list/tree is active. An active list/tree has keyboard focus, an inactive does not.
		"list.highlightForeground": "#ff0000", // List/Tree foreground color of the match highlights when searching inside the list/tree.
		"list.hoverBackground": "#ff0000", // List/Tree background when hovering over items using the mouse.
		"list.hoverForeground": "#ff0000", // List/Tree foreground when hovering over items using the mouse.
		"list.inactiveSelectionBackground": "#ff0000", // List/Tree background color for the selected item when the list/tree is inactive.
		"list.inactiveSelectionForeground": "#ff0000", // List/Tree foreground color for the selected item when the list/tree is inactive. An active list/tree has keyboard focus, an inactive does not.
		"list.inactiveSelectionIconForeground": "#ff0000", // List/Tree icon foreground color for the selected item when the list/tree is inactive. An active list/tree has keyboard focus, an inactive does not.
		"list.inactiveFocusBackground": "#ff0000", // List background color for the focused item when the list is inactive. An active list has keyboard focus, an inactive does not. Currently only supported in lists.
		"list.inactiveFocusOutline": "#ff0000", // List/Tree outline color for the focused item when the list/tree is inactive. An active list/tree has keyboard focus, an inactive does not.
		"list.invalidItemForeground": "#ff0000", // List/Tree foreground color for invalid items, for example an unresolved root in explorer.
		"list.errorForeground": "#ff0000", // Foreground color of list items containing errors.
		"list.warningForeground": "#ff0000", // Foreground color of list items containing warnings.
		"listFilterWidget.background": "#ff0000", // List/Tree Filter background color of typed text when searching inside the list/tree.
		"listFilterWidget.outline": "#ff0000", // List/Tree Filter Widget's outline color of typed text when searching inside the list/tree.
		"listFilterWidget.noMatchesOutline": "#ff0000", // List/Tree Filter Widget's outline color when no match is found of typed text when searching inside the list/tree.
		"list.filterMatchBackground": "#ff0000", // Background color of the filtered matches in lists and trees.
		"list.filterMatchBorder": "#ff0000", // Border color of the filtered matches in lists and trees.
		"list.deemphasizedForeground": "#ff0000", // List/Tree foreground color for items that are deemphasized.
		"tree.indentGuidesStroke": "#ff0000", // Tree Widget's stroke color for indent guides.
		"tree.tableColumnsBorder": "#ff0000", // Tree stroke color for the indentation guides.
		"tree.tableOddRowsBackground": "#ff0000", // Background color for odd table rows.

		// Active Bar
		"activityBar.background": "#ff0000", // Activity Bar background color.
		"activityBar.dropBorder": "#ff0000", // Drag and drop feedback color for the activity bar items. The activity bar is showing on the far left or right and allows to switch between views of the side bar.
		"activityBar.foreground": "#ff0000", // Activity Bar foreground color (for example used for the icons).
		"activityBar.inactiveForeground": "#ff0000", // Activity Bar item foreground color when it is inactive.
		"activityBar.border": "#ff0000", // Activity Bar border color with the Side Bar.
		"activityBarBadge.background": "#ff0000", // Activity notification badge background color.
		"activityBarBadge.foreground": "#ff0000", // Activity notification badge foreground color.
		"activityBar.activeBorder": "#ff0000", // Activity Bar active indicator border color.
		"activityBar.activeBackground": "#ff0000", // Activity Bar optional background color for the active element.
		"activityBar.activeFocusBorder": "#ff0000", // Activity bar focus border color for the active item.

		// Side Bar
		"sideBar.background": "#ff0000", // Side Bar background color.
		"sideBar.foreground": "#ff0000", // Side Bar foreground color. The Side Bar is the container for views like Explorer and Search.
		"sideBar.border": "#ff0000", // Side Bar border color on the side separating the editor.
		"sideBar.dropBackground": "#ff0000", // Drag and drop feedback color for the side bar sections. The color should have transparency so that the side bar sections can still shine through.
		"sideBarTitle.foreground": "#ff0000", // Side Bar title foreground color.
		"sideBarSectionHeader.background": "#ff0000", // Side Bar section header background color.
		"sideBarSectionHeader.foreground": "#ff0000", // Side Bar section header foreground color.
		"sideBarSectionHeader.border": "#ff0000", // Side bar section header border color.

		// Minimap
		"minimap.findMatchHighlight": "#ff0000", // Highlight color for matches from search within files.
		"minimap.selectionHighlight": "#ff0000", // Highlight color for the editor selection.
		"minimap.errorHighlight": "#ff0000", // Highlight color for errors within the editor.
		"minimap.warningHighlight": "#ff0000", // Highlight color for warnings within the editor.
		"minimap.background": "#ff0000", // Minimap background color.
		"minimap.selectionOccurrenceHighlight": "#ff0000", // Minimap marker color for repeating editor selections.
		"minimap.foregroundOpacity": "#ff0000", // Opacity of foreground elements rendered in the minimap. For example, "#000000c0" will render the elements with 75% opacity.
		"minimap.unicodeHighlight": "#ff0000", // Minimap marker color for highlighted unicode characters.
		"minimapSlider.background": "#ff0000", // Minimap slider background color.
		"minimapSlider.hoverBackground": "#ff0000", // Minimap slider background color when hovering.
		"minimapSlider.activeBackground": "#ff0000", // Minimap slider background color when clicked on.
		"minimapGutter.addedBackground": "#ff0000", // Minimap gutter color for added content.
		"minimapGutter.modifiedBackground": "#ff0000", // Minimap gutter color for modified content.
		"minimapGutter.deletedBackground": "#ff0000", // Minimap gutter color for deleted content.

		// Editor Groups & Tabs
		"editorGroup.border": "#ff0000", // Color to separate multiple editor groups from each other.
		"editorGroup.dropBackground": "#ff0000", // Background color when dragging editors around.
		"editorGroupHeader.noTabsBackground": "#ff0000", // Background color of the editor group title header when Tabs are disabled (set "workbench.editor.showTabs": false).
		"editorGroupHeader.tabsBackground": "#ff0000", // Background color of the Tabs container.
		"editorGroupHeader.tabsBorder": "#ff0000", // Border color below the editor tabs control when tabs are enabled.
		"editorGroupHeader.border": "#ff0000", // Border color between editor group header and editor (below breadcrumbs if enabled).
		"editorGroup.emptyBackground": "#ff0000", // Background color of an empty editor group.
		"editorGroup.focusedEmptyBorder": "#ff0000", // Border color of an empty editor group that is focused.
		"tab.activeBackground": "#ff0000", // Active Tab background color in an active group.
		"tab.unfocusedActiveBackground": "#ff0000", // Active Tab background color in an inactive editor group.
		"tab.activeForeground": "#ff0000", // Active Tab foreground color in an active group.
		"tab.border": "#ff0000", // Border to separate Tabs from each other.
		"tab.activeBorder": "#ff0000", // Bottom border for the active tab.
		"tab.unfocusedActiveBorder": "#ff0000", // Bottom border for the active tab in an inactive editor group.
		"tab.activeBorderTop": "#ff0000", // Top border for the active tab.
		"tab.unfocusedActiveBorderTop": "#ff0000", // Top border for the active tab in an inactive editor group
		"tab.lastPinnedBorder": "#ff0000", // Border on the right of the last pinned editor to separate from unpinned editors.
		"tab.inactiveBackground": "#ff0000", // Inactive Tab background color.
		"tab.unfocusedInactiveBackground": "#ff0000", // Inactive Tab background color in an unfocused group
		"tab.inactiveForeground": "#ff0000", // Inactive Tab foreground color in an active group.
		"tab.unfocusedActiveForeground": "#ff0000", // Active tab foreground color in an inactive editor group.
		"tab.unfocusedInactiveForeground": "#ff0000", // Inactive tab foreground color in an inactive editor group.
		"tab.hoverBackground": "#ff0000", // Tab background color when hovering
		"tab.unfocusedHoverBackground": "#ff0000", // Tab background color in an unfocused group when hovering
		"tab.hoverForeground": "#ff0000", // Tab foreground color when hovering
		"tab.unfocusedHoverForeground": "#ff0000", // Tab foreground color in an unfocused group when hovering
		"tab.hoverBorder": "#ff0000", // Border to highlight tabs when hovering
		"tab.unfocusedHoverBorder": "#ff0000", // Border to highlight tabs in an unfocused group when hovering
		"tab.activeModifiedBorder": "#ff0000", // Border on the top of modified (dirty) active tabs in an active group.
		"tab.inactiveModifiedBorder": "#ff0000", // Border on the top of modified (dirty) inactive tabs in an active group.
		"tab.unfocusedActiveModifiedBorder": "#ff0000", // Border on the top of modified (dirty) active tabs in an unfocused group.
		"tab.unfocusedInactiveModifiedBorder": "#ff0000", // Border on the top of modified (dirty) inactive tabs in an unfocused group.
		"editorPane.background": "#ff0000", // Background color of the editor pane visible on the left and right side of the centered editor layout.
		"sideBySideEditor.horizontalBorder": "#ff0000", // Color to separate two editors from each other when shown side by side in an editor group from top to bottom.
		"sideBySideEditor.verticalBorder": "#ff0000", // Color to separate two editors from each other when shown side by side in an editor group from left to right.

		// Editor colors
		"editor.background": "#ff0000", // Editor background color.
		"editor.foreground": "#ff0000", // Editor default foreground color.
		"editorLineNumber.foreground": "#ff0000", // Color of editor line numbers.
		"editorLineNumber.activeForeground": "#ff0000", // Color of the active editor line number.
		"editorCursor.background": "#ff0000", // The background color of the editor cursor. Allows customizing the color of a character overlapped by a block cursor.
		"editorCursor.foreground": "#ff0000", // Color of the editor cursor.

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#23c280"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "JSON Key - Level 1",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "JSON Key - Level 2",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "JSON Key - Level 3",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#FF5370"
			}
